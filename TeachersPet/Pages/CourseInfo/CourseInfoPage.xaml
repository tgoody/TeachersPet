      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:TeachersPet.Pages.CourseInfo"
      mc:Ignorable="d"
      Title="CourseInfo" Height="600" Width="900">
    <Grid>
        <TextBlock Name="CourseName" FontFamily="Bahnschrift" FontSize="28"/>
        <TextBlock Name="CourseInfo" FontFamily="Bahnschrift Light" FontSize="24" Margin="0,40,0,0"/>
        <WrapPanel Name="WrapPanel" Orientation="Vertical" Margin="0,75,0,0"/>
    </Grid>
</Page>
